 <section>
    <div class="container">
      <div class="row">
        <div class="col-12 mt-4">
        <% if !flash.empty? %>
          <% flash.each do |name, msg| %>
            <ul>
              <li><%= msg %></li>
            </ul>
          <% end %>
        <% end %>
        </div>
      </div>
    </div>
  </section>